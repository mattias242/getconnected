{
  "version": 2,
  "name": "getconnected",
  "builds": [
    {
      "src": "src/web/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/web/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/web/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/web/server.js": {
      "maxDuration": 30
    }
  }
}